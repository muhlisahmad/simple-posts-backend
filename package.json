{
  "name": "takehome-test-backend",
  "version": "1.0.0",
  "description": "Backend RESTful API take home test",
  "main": "server.js",
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "scripts": {
    "start:prod": "SET NODE_ENV=production && nodemon --exec \"tsc && node -r dotenv/config ./dist/server.js\"",
    "start:dev": "SET NODE_ENV=development && nodemon --exec \"node --loader ts-node/esm -r dotenv/config ./src/server.ts\"",
    "build": "tsc",
    "start": "node ./dist/server.js"
  },
  "type": "module",
  "author": "Muhlis Ahmad Abdillah",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.4.1",
    "nodemon": "^3.1.4",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  }
}
